!=================================================

! Diretorio de execucao
dirRun = .//Runs

! Diretorio do experimnto
dirExperiment = .//Baseline//Phenology

! Diretorio do DSSAT
dssatFile = .//bin//dscsm047

! Arquivo de cultivares
cultivarFile = .//Baseline//Phenology//BNGRO047.CUL

!=================================================

! Diretorio de saida (saida de resultados finais)
outputDir = .//output

!=================================================

! Identificador do cultivar
cultivar = IB0096

! Identificador do modelo
model = CRGRO047

! Coeficientes de analise
! Variaveis problematicas = THRSH
!coefficients = EM-FL, FL-SD, SD-PM, XFRT, THRSH, SDPRO
coefficients = CSDL, PPSEN
!, EM-FL, FL-SH, FL-SD, SD-PM, FL-LF, LFMAX, SLAVR, SIZLF,  XFRT, WTPSD, SFDUR, SDPDV, PODUR, SDPRO, SDLIP

! Variaveis de calibracao
!calibration = ADAP, MDAP, PD1P, PDFP
calibration = ADAP, MDAP, PD1P, PDFP, LAID, SWAD, GWAD, LWAD

! Quanidade de interacoes
maxiter = 30

! Tamanho da populacao
popSize = 30

! Executar simulacao em paralelo (1 para sequencial)
simulationCores = 4

!=================================================
